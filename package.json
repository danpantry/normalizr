{
  "name": "@danpantry/normalizr-immutable",
  "version": "4.0.0",
  "description":
    "Normalizes and denormalizes JSON according to schema for Redux and Flux applications using ImmutableJS",
  "homepage": "https://github.com/danpantry/normalizr",
  "repository": {
    "url": "https://github.com/danpantry/normalizr.git",
    "type": "git"
  },
  "keywords": ["flux", "redux", "normalize", "denormalize", "api", "json"],
  "files": ["dist/", "index.d.ts", "LICENSE", "README.md"],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "typings": "index.d.ts",
  "sideEffects": false,
  "jest": {
    "testURL": "http://localhost"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:cjs": "tsc -p tsconfig.json",
    "build:es": "tsc -p tsconfig.es.json",
    "clean": "rimraf dist es",
    "prebuild": "npm run clean",
    "precommit": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:coverage": "npm run test -- --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "author": "Paul Armstrong",
  "contributors": ["Dan Abramov", "Dan Pantry"],
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^2.11.15",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest": "24",
    "lint-staged": "^6.1.0",
    "npm-run-all": "^3.1.2",
    "prettier": "^1.12.1",
    "rimraf": "^2.5.4",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.2"
  },
  "lint-staged": {
    "src/**/*.js": ["test", "git add"],
    "*.{js,json,css,md}": ["prettier --write", "git add"]
  },
  "peerDependencies": {
    "immutable": "^4.0.0-rc.12"
  },
  "private": false,
  "browserslist": [">0.2%", "not dead", "not ie <= 11", "not op_mini all"]
}
